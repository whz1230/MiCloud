CREATE TABLE "DB2ADMIN"."T_FILE"
 ("ID"           VARCHAR(32)           NOT NULL,
  "NAME"         VARCHAR(200),
  "SERVER_NAME"  VARCHAR(200),
  "TYPE"         VARCHAR(10),
  "DESCRITION"   VARCHAR(200),
  "UPLOAD_TIME"  VARCHAR(19),
  "FILE_DATA"    BLOB(1073741824)                LOGGED  NOT COMPACT
 )
  DATA CAPTURE NONE
 IN "USERSPACE1";

ALTER TABLE "DB2ADMIN"."T_FILE"
  LOCKSIZE ROW
  APPEND OFF
  NOT VOLATILE
  LOG INDEX BUILD NULL;

ALTER TABLE "DB2ADMIN"."T_FILE"
  ADD CONSTRAINT "SQL170114140239450" PRIMARY KEY
   ("ID"
   );

