<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.mi.vo">

	<class name="Category" table="CATEGORY">
		<id name="id">
			<generator class="uuid" />
		</id>
		<property name="name" column="NAME" type="string" not-null="false" length="200" />
		<property name="lft" column="LFT" type="integer" not-null="false" length="10" />
		<property name="rgt" column="RGT" type="integer" not-null="false" length="10" />


	</class>

</hibernate-mapping>
