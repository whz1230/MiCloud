<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.mi.vo">

	<class name="FileVo" table="T_FILE">
		<id name="id">
			<generator class="uuid" />
		</id>
		<property name="fileName" column="FILENAME" type="string" not-null="false" length="200" />
		<property name="serverName" column="SERVERNAME" type="string" not-null="false" length="200" />
		<property name="type" column="TYPE" type="string" not-null="false" length="10" />
		<property name="description" column="DESCRIPTION" type="string" not-null="false" length="200" />
		<property name="uploadTime" column="UPLOADTIME" type="string" not-null="false" length="19" />
		<property name="fileData" column="FILEDATA" type="binary" not-null="false" />


	</class>

</hibernate-mapping>
